												"Domain": {
													"value": "chakri.database.windows.net",
													"type": "Expression"
												},
												"Database": {
													"value": "@item().TABLE_CATALOG",
													"type": "Expression"
												},
												"Username": {
													"value": "@pipeline().parameters.username",
													"type": "Expression"
												},
												"Password": {
													"value": "@pipeline().parameters.Password",
