												},
												"TableName": {
													"value": "@item().TABLE_NAME",
													"type": "Expression"
												}
											}
										}
									],
									"outputs": [
										{
											"referenceName": "Dynamic_output",
											"type": "DatasetReference",
											"parameters": {
												"Container_output": "chakri",
												"Directory_output": "Bulk_copied_folder",
												"File_output": {
													"value": "@item().TABLE_NAME",
													"type": "Expression"
												}
											}
										}
									]
								}
							]
						}
					}
