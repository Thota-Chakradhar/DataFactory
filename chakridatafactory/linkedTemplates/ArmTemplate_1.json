									},
									"Directory_input": {
										"value": "@pipeline().parameters.Directory_input",
										"type": "Expression"
									},
									"File_input": {
										"value": "@pipeline().parameters.File_input",
										"type": "Expression"
									},
									"Sheet_name": {
										"value": "@pipeline().parameters.Sheet_name",
										"type": "Expression"
									}
								}
							}
						],
						"outputs": [
							{
